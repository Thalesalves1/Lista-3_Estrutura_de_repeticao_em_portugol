Algoritmo "Tabuada"

//Thales Alves
//CPD:50629
Var
   num1, numero: inteiro

Inicio

   escreval("Digite um numero: ")
   leia(numero)

   escreval (" Tabuada de",numero," ")

   para num1 de 1 ate 9 faca
      escreval(numero," x",num1," = ",(numero* num1))
   fimpara

fimalgoritmo