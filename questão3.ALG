Algoritmo "Calculos"
//Thales Alves
//CPD: 50629
Var
   num1,num2,result: real


Inicio

   num1 <- 0

   escreval("Digite um número:")
   leia(num2)
   
   num1 <-((1 + 99)*99)/2
   num2 <-((1+50)*50)/2
   
   result<- num1/num2
   
   escreval("O resultado é: ",num1,num2,result)
   
   
fimalgoritmo